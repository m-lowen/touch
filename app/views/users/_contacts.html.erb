<div class="panel2">
  <!-- Default panel contents -->
  <div class="panel-heading contact_heading"><h2><u>Contacts</u></h2></div>

  <!-- List group -->
  <ul class="list-group">
	<% @contacts.each do |contact| %>
    <li class="list-group-item reminders-list"><b><%= link_to contact.name, contact_path(contact.id) %></b></li>
    <% end %>
  </ul>
  </div>